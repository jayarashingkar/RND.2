@model RNDSystems.Models.ViewModels.ImportDataViewModel
@{
    ViewBag.Title = "SCCResult";
}

<div class="row">
    <div class="form-group">
        <label for="lblStressKsi" class="cols-sm-2 control-label">StressKsi</label>
        <div class="cols-sm-10">
            <div class="input-group">
                <span class="input-group-addon"><i class="fa fa-text-width" aria-hidden="true"></i></span>
                <input type="text" id="StressKsi" name="StressKsi" class="form-control" placeholder="Enter StressKsi" />
            </div>
        </div>
    </div>
</div>
<div class="row">
    <div class="form-group">
        <label for="lblTimeDays" class="cols-sm-2 control-label">TimeDays</label>
        <div class="cols-sm-10">
            <div class="input-group">
                <span class="input-group-addon"><i class="fa fa-text-width" aria-hidden="true"></i></span>
                <input type="text" id="TimeDays" name="TimeDays" class="form-control" placeholder="Enter TimeDays" />

            </div>
        </div>
    </div>
</div>
<div class="row">
    <div class="form-group">
        <label for="lblTestStatus" class="cols-sm-2 control-label">TestStatus</label>
        <div class="cols-sm-10">
            <div class="input-group">
                <span class="input-group-addon"><i class="fa fa-text-width" aria-hidden="true"></i></span>
                <input type="text" id="TestStatus" name="TestStatus" class="form-control" placeholder="Enter TestStatus" />

            </div>
        </div>
    </div>
</div>
<div class="row">
    <div class="form-group">
        <label for="lblSCCSpeciComment" class="cols-sm-2 control-label">SpeciComment</label>
        <div class="cols-sm-10">
            <div class="input-group">
                <span class="input-group-addon"><i class="fa fa-text-width" aria-hidden="true"></i></span>
                <input type="text" id="SCCSpeciComment" name="SCCSpeciComment" class="form-control" placeholder="Enter SpeciComment" />

            </div>
        </div>
    </div>
</div>
<div class="row">
    <div class="form-group">
        <label for="lblSCCOperator" class="cols-sm-2 control-label">Operator</label>
        <div class="cols-sm-10">
            <div class="input-group">
                <span class="input-group-addon"><i class="fa fa-text-width" aria-hidden="true"></i></span>
                <input type="text" id="SCCOperator" name="SCCOperator" class="form-control" placeholder="Enter Operator" />

            </div>
        </div>
    </div>
</div>
<div class="row">
 
        <div class="form-group">
            <label for="lblTestStartedDate" class="cols-sm-10 control-label"> Test Start Date</label>
            <div class="cols-sm-10">
                <div class="input-group date">
                    <input type="text" id="TestStartDate" name="TestStartDate" class="form-control"><span class="input-group-addon"><i class="glyphicon glyphicon-th"></i></span>
                </div>
            </div>
        </div>
   
</div>
<div class="row">
 
        <div class="form-group">
            <label for="lblTestEndDate" class="cols-sm-10 control-label">Test End Date</label>
            <div class="cols-sm-10">
                <div class="input-group date">
                    <input type="text" id="TestEndDate" name="TestEndDate" class="form-control"><span class="input-group-addon"><i class="glyphicon glyphicon-th"></i></span>
                </div>
            </div>
        </div>
 
</div>


